<h2>Movies Added by Me</h2>
<br>
<br>
<table align="center">
    <tr>
        <th>Title</th>
        <th>Year</th>
        <th>Director</th>
    </tr>
    {{#each userMovies}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.year}}</td>
            <td>{{this.director}}</td>
        </tr>
    {{/each}}
</table>
<a href="/movies">Home</a>